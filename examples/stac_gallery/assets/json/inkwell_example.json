{
    "type": "scaffold",
    "appBar": {
        "type": "appBar",
        "title": {
            "type": "text",
            "data": "InkWell"
        }
    },
    "body": {
        "type": "listView",
        "shrinkWrap": true,
        "children": [
            {
                "type": "center",
                "child": {
                    "type": "inkWell",
                    "child": {
                        "type": "padding",
                        "padding": {
                            "top": 20,
                            "bottom": 20,
                            "right": 20,
                            "left": 20
                        },
                        "child": {
                            "type": "text",
                            "data": "Hello, World! from Inkwell",
                            "textAlign": "center"
                        }
                    },
                    "splashColor": "#E1BEE7",
                    "borderRadius": {
                        "topLeft": 20,
                        "topRight": 20,
                        "bottomLeft": 20,
                        "bottomRight": 20
                    },
                    "radius": 20,
                    "hoverDuration": {
                        "seconds": 10
                    },
                    "onTap": {
                        "actionType": "showDialog",
                        "widget": {
                            "type": "alertDialog",
                            "title": {
                                "type": "text",
                                "data": "On Tap Successful"
                            }
                        }
                    }
                }
            }
        ]
    }
}