              {
                  "type": "padding",
                  "padding": 16,
                  "child": {
                      "type": "text",
                      "data": "Frosted Glass Effect with sigmaX: 15.0, sigmaY: 15.0",
                      "style": {
                          "fontSize": 18
                      }
                  }
              },
              {
                "type": "clipRRect",
