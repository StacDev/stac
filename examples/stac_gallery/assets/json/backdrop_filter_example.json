                          }
                        }
                      },
                      {
                        "type": "backdropFilter",
                        "filter": {
                          "type": "dilate",
                          "radiusX": 2.0,
                          "radiusY": 2.0
                        },
                        "child": {
                          "type": "container",
                          "decoration": {
                            "color": "#80FFFFFF"
                          }
                        }
                      }
                    ]
                  }
                }
              },
              {
                  "type": "divider",
                  "height": 20
              },
              {
                  "type": "padding",
                  "padding": 16,
                  "child": {
                      "type": "text",
